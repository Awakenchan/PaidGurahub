    Tab:Toggle{
    	Name = "Item Farm",
    	StartingState = _G.Settings.ActiveItemFarm,
    	Description = nil,
    	Callback = function(state)
    		_G.Settings.ActiveItemFarm = state
    		saveSettings()
    
    		getgenv().FarmItem = _G.Settings.ActiveItemFarm
    
    		spawn(function()
    		    while getgenv().FarmItem do
    		    	if getgenv().FarmArrow == true then
    		    		if game:GetService("Workspace").Items:FindFirstChild("Stand Arrow") then
    		    			cf = game:GetService("Workspace").Items:FindFirstChild("Stand Arrow").Handle.Position
    		    			game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(tweenspeed, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {CFrame = CFrame.new(cf)}):Play()
    		    			wait(tweenspeed)
    		    		end
    		    	end
    		    	if getgenv().FarmRokakaka == true then
    		    		if game:GetService("Workspace").Items:FindFirstChild("Rokakaka") then
    		    			cf = game:GetService("Workspace").Items:FindFirstChild("Rokakaka").Handle.Position
    		    			game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(tweenspeed, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {CFrame = CFrame.new(cf)}):Play()
    		    			wait(tweenspeed)
    		    		end
    		    	end
    		    	if getgenv().FarmChargedArrow == true then
    		    		if game:GetService("Workspace").Items:FindFirstChild("Charged Arrow") then
    		    			cf = game:GetService("Workspace").Items:FindFirstChild("Charged Arrow").Handle.Position
    		    			game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(tweenspeed, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {CFrame = CFrame.new(cf)}):Play()
    		    			wait(tweenspeed)
    		    		end
    		    	end
    		    	if getgenv().FarmRequiemArrow == true then
    		    		if game:GetService("Workspace").Items:FindFirstChild("Requiem Arrow") then
    		    			cf = game:GetService("Workspace").Items:FindFirstChild("Requiem Arrow").Handle.Position
    		    			game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(tweenspeed, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {CFrame = CFrame.new(cf)}):Play()
    		    			wait(tweenspeed)
    		    		end
