    	--spawn(function()
    		local RunService = game:GetService("RunService")
            while getgenv().MobActiveFarm == true do
                for i, v in pairs(game:GetService("Workspace").Living:GetChildren()) do
                    if v.Name == getgenv().Mob then
                        if v:FindFirstChild("Humanoid") then
                            if v:FindFirstChild("Humanoid").Health > 0 then
                                if game:GetService("Players").LocalPlayer.Character then
                                    if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Humanoid") then
            							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0,0,getgenv().Distance)
    	    						end
                				end
                			end
                		end
                	end
                end
    			wait()
            end
    	--end)
    end
