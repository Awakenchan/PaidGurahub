    		    			cf = game:GetService("Workspace").Items:FindFirstChild("Green Baby").Handle.Position
    		    			game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(tweenspeed, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {CFrame = CFrame.new(cf)}):Play()
    		    			wait(tweenspeed)
    		    		end
    		    	end
    		    	if getgenv().FarmDiosDiary == true then
    		    		if game:GetService("Workspace").Items:FindFirstChild("Dio's Diary") then
    		    			cf = game:GetService("Workspace").Items:FindFirstChild("Dio's Diary").Handle.Position
