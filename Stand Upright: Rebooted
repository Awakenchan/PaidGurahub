    				    if v:IsA("RemoteEvent") then
    				    	if v.Name == "Summon" then
    				    	elseif v.Name == "Quote" then
    				    	elseif v.Name == "Pose" then
    				    	elseif v.Name == "M1" then
    				    	elseif v.Name == "Block" then
    				    	elseif v.Name == "TogglePilot" then
    				    	else
    				    		v:FireServer()
    				    	end
    				    end
					end
