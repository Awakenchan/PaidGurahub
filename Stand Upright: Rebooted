    Tab:Toggle{
    	Name = "Auto Get Quest",
    	StartingState = false,
    	Description = nil,
    	Callback = function(state)
    		getgenv().GetQuest = state
    
    		while getgenv().GetQuest do
    			if not game:GetService("Players").LocalPlayer.Data.Quests:FindFirstChild(getgenv().Quest) then
    				if getgenv().Quest == "KillBadGi" then
    					game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["4"].Done:FireServer()
    				elseif getgenv().Quest == "KillGiorno" then
    					game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["5"].Done:FireServer()
    				elseif getgenv().Quest == "KillMonster" then
    					game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["6"].Done:FireServer()
    				elseif getgenv().Quest == "KillRker312" then
    					game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["7"].Done:FireServer()
    				elseif getgenv().Quest == "KillDioOH" then
    					game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["8"].Done:FireServer()
    				elseif getgenv().Quest == "KillKira" then
    					game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["9"].Done:FireServer()
    				elseif getgenv().Quest == "KillAngelo" then
    					game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["10"].Done:FireServer()
    				end -- Seems to work if i do that
    				if getgenv().Quest == "KillAlien" then
    					game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["11"].Done:FireServer()
    				elseif getgenv().Quest == "KillJotarop4" then
    			    	game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["12"].Done:FireServer()
    			    elseif getgenv().Quest == "KillKakyoin" then
    			    	game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["13"].Done:FireServer()
    				elseif getgenv().Quest == "KillJungleGi" then
    					game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["14"].Done:FireServer()
    				elseif getgenv().Quest == "KillSewervampires" then
    					game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["32"].Done:FireServer()
    				end
    			elseif game:GetService("Players").LocalPlayer.Data.Quests:FindFirstChild(getgenv().Quest) then
    				if game:GetService("Players").LocalPlayer.Data.Quests[getgenv().Quest].Completed.Value == true then
    					if getgenv().Quest == "KillBadGi" then
    						game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["4"].QuestDone:FireServer()
    					elseif getgenv().Quest == "KillGiorno" then
    						game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["5"].QuestDone:FireServer()
    					elseif getgenv().Quest == "KillMonster" then
    						game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["6"].QuestDone:FireServer()
    					elseif  getgenv().Quest == "KillRker312" then
    						game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["7"].QuestDone:FireServer()
    					elseif  getgenv().Quest == "KillDioOH" then
    						game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["8"].QuestDone:FireServer()
    					elseif  getgenv().Quest == "KillKira" then
    						game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["9"].QuestDone:FireServer()
    					elseif getgenv().Quest == "KillAngelo" then
    						game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["10"].QuestDone:FireServer()
    					end
    					if getgenv().Quest == "KillAlien" then
    						game:GetService("Workspace")["hello exploiter why u see the Annoying Orange"]["11"].QuestDone:FireServer()
    					elseif getgenv().Quest == "KillJotarop4" then
